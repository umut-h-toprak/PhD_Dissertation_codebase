rm -rf sophiaMref
mkdir -p sophiaMref/src
cd sophiaMref

g++ -std=c++1z -I"../include" -O3 -Wall -Wextra -static -static-libgcc -static-libstdc++ -flto -c -fmessage-length=0 -Wno-attributes -MMD -MP -MF"src/MasterRefProcessor.d" -MT"src/MasterRefProcessor.o" -o "src/MasterRefProcessor.o" "../src/MasterRefProcessor.cpp"
