g++ -static -static-libgcc -static-libstdc++ -flto -o "sophiaAnnotate"  ./src/AnnotationProcessor.o ./src/Breakpoint.o ./src/BreakpointReduced.o ./src/ChrConverter.o ./src/DeFuzzier.o ./src/GermlineMatch.o ./src/MrefEntry.o ./src/MrefEntryAnno.o ./src/MrefMatch.o ./src/SuppAlignment.o ./src/SuppAlignmentAnno.o ./src/SvEvent.o  ./sophiaAnnotate.o   -lz -lboost_system -lboost_iostreams
