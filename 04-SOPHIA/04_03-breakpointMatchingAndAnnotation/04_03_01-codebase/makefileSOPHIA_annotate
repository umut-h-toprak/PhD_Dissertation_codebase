rm -rf sophiaAnnotate
mkdir -p sophiaAnnotate/src
cd sophiaAnnotate
g++ -std=c++1z -I"../include" -O3 -Wall -Wextra -static -static-libgcc -static-libstdc++ -flto -c -fmessage-length=0 -Wno-attributes -MMD -MP -MF"src/AnnotationProcessor.d" -MT"src/AnnotationProcessor.o" -o "src/AnnotationProcessor.o" "../src/AnnotationProcessor.cpp"
