g++ -std=c++1z -I"../include" -O3 -Wall -Wextra -static -static-libgcc -static-libstdc++ -flto -c -fmessage-length=0 -Wno-attributes -MMD -MP -MF"src/SuppAlignment.d" -MT"src/SuppAlignment.o" -o "src/SuppAlignment.o" "../src/SuppAlignment.cpp"
